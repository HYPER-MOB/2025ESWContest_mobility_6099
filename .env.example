# AWS Configuration
AWS_REGION=ap-northeast-2
AWS_PROFILE=default

# Database Configuration
DB_HOST=your-rds-endpoint.amazonaws.com
DB_NAME=hypermob
DB_USER=your_username
DB_PASSWORD=your_password
DB_PORT=3306

# S3 Buckets
S3_IMAGES_BUCKET=hypermob-images
S3_RESULTS_BUCKET=hypermob-results

# Lambda Settings
LAMBDA_TIMEOUT=60
LAMBDA_MEMORY_SIZE=1024

# API Gateway
API_GATEWAY_NAME=HYPERMOB-Unified-API
API_GATEWAY_STAGE=v1